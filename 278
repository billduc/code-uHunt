#include <bits/stdc++.h>

using namespace std;

#define X first
#define Y second
#define pb push_back
#define mp make_pair
#define sz(a) ((int)(a).size())
#define all(a) (a).begin(),(a).end()
#define DEBUG(x) {cout<< #x <<" = " << x <<endl;}

typedef long long ll;
typedef vector<int> vi;
typedef pair<int,int> ii;


int main(){
    //freopen("in.txt","r",stdin);
    //freopen("out.txt","w",stdout);
    //ios::sync_with_stdio(false);
    int test;
    cin >> test;
    while (test--){
        char c;
        int m,n;
        cin >> c >> m >> n;
        switch(c){
            case 'r': case'Q':
                cout<<min(m,n)<<endl;
                break;
            case 'k':
                cout << (int)((n*m)+1)/2 <<endl;
                break;
            case 'K':
                    cout << ((m+1)/2)*((n+1)/2) << endl;
        }
    }

    return 0;
}
